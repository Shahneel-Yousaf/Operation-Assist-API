{
  "name": "nestjs-starter-rest-api",
  "private": true,
  "version": "1.0.0",
  "description": "NestJS Starter REST API Repository",
  "license": "MIT",
  "contributors": [
    "Yash Murty <yashmurty@gmail.com> (https://github.com/yashmurty)",
    "S M Asad Rahman<asad.rahman@monstar-lab.com> (https://github.com/asad-mlbd)",
    "Tanveer Hassan <mailtanveerhassan@gmail.com> (https://github.com/war1oc)",
    "Saad Bin Amjad <saadbinamjad@gmail.com> (https://github.com/Saad-Amjad)",
    "Sivan Payyadakath <sivanp77@gmail.com > (https://github.com/sivanpayyadakath)"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "typeorm": "typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate -d ormconfig.ts migrations/$npm_config_name",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run -d ormconfig.ts",
    "migration:run:test": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm migration:run -d ormconfig.test.ts",
    "migration:run:prod": "node node_modules/.bin/typeorm migration:run -d ./dist/ormconfig.js",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert -d ormconfig.ts",
    "doc:serve": "npx compodoc -p tsconfig.json -w -s -r 8001",
    "doc:generate": "npx compodoc -p tsconfig.json",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.ts": [
      "npm run lint:fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "@azure/msal-node": "^2.6.3",
    "@azure/storage-blob": "^12.16.0",
    "@hapi/joi": "^17.1.1",
    "@nestjs/cache-manager": "^2.2.0",
    "@nestjs/common": "^9.1.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.1.1",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/platform-express": "^9.1.1",
    "@nestjs/swagger": "^7.1.11",
    "@nestjs/typeorm": "^10.0.0",
    "@types/uuid": "^9.0.4",
    "axios": "^1.6.7",
    "cache-manager": "^5.3.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "firebase-admin": "^12.0.0",
    "jwks-rsa": "^3.1.0",
    "mssql": "^9.1.1",
    "nestjs-i18n": "^10.3.5",
    "puppeteer": "^21.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.3",
    "rxjs": "^7.5.6",
    "semver": "^7.6.0",
    "typeorm": "^0.3.17",
    "ulid": "^2.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^19.5.0",
    "@compodoc/compodoc": "^1.1.19",
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.1",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.6",
    "@types/node": "^18.7.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@types/puppeteer": "^7.0.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.8",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      ".module.ts$"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 60000,
    "moduleNameMapper": {
      "^@shared(.*)$": "<rootDir>/shared$1",
      "^@user(.*)$": "<rootDir>/user$1",
      "^@group(.*)$": "<rootDir>/group$1",
      "^@user-group-role-template(.*)$": "<rootDir>/user-group-role-template$1",
      "^@blob-storage(.*)$": "<rootDir>/blob-storage$1",
      "^@machine(.*)$": "<rootDir>/machine$1",
      "^@machine-report(.*)$": "<rootDir>/machine-report$1",
      "^@template(.*)$": "<rootDir>/template$1",
      "^@inspection(.*)$": "<rootDir>/inspection$1",
      "^@firebase(.*)$": "<rootDir>/firebase$1",
      "^@graph-api(.*)$": "<rootDir>/graph-api$1"
    }
  }
}
